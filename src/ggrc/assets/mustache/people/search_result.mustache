{{#list}}
<li class="{{class.category}}" data-model="true">
  <a href="{{viewLink}}">
    <div>
      <div class="lhs-main-title">
        <span class="lhs-item lhs-item-long">
          {{firstnonempty title name email}}
        </span>
      </div>
    </div>
  </a>
</li>
{{/list}}

{{#list.is_loading}}
  <li class="spinny">
    <spinner toggle="list.is_loading"></spinner>
  </li>
{{/list.is_loading}}
