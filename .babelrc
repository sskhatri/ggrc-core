{
  "env": {
    "production": {
      "plugins": [
        "transform-runtime",
        "syntax-dynamic-import",
        "transform-object-rest-spread",
        "transform-strict-mode"
      ],
      "presets": [
        [
          "env",
          {
            "targets": {
              "browsers": [
                "last 2 versions"
              ]
            },
            "modules": false
          }
        ]
      ],
      "minified": true,
      "comments": false,
      "compact": true
    },
    "development": {
      "plugins": [
        "transform-runtime",
        "syntax-dynamic-import",
        "transform-object-rest-spread",
        "transform-strict-mode"
      ],
      "presets": [
        [
          "env",
          {
            "debug": true,
            "targets": {
              "browsers": [
                "last 2 versions"
              ]
            }
          }
        ]
      ]
    }
  }
}